{
  "name": "anya_portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:dzemyanovich/anya_portfolio.git",
  "author": "Aliaksei Dzemyanovich <aliaksei.dzemyanovich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "local": "webpack-dev-server --mode=development --config webpack.local.js --open",
    "dev": "webpack-dev-server --mode=production --config webpack.dev.js --open",
    "preprod": "webpack-dev-server --mode=production --config webpack.preprod.js --open",
    "prod1": "webpack-dev-server --mode=production --config webpack.prod.js --open",
    "build-local": "webpack --mode=development --config webpack.local.js",
    "build-dev": "webpack --mode=production --config webpack.dev.js",
    "build-preprod": "webpack --mode=production --config webpack.preprod.js",
    "build-prod": "webpack --mode=production --config webpack.prod.js",
    "tg-plan": "yarn zip && yarn build-dev && cd terragrunt/live/dev && terragrunt run-all plan",
    "tg-plan-pure": "cd terragrunt/live/dev && terragrunt run-all plan",
    "tg-apply": "yarn zip && yarn build-dev && cd terragrunt/live/dev && terragrunt run-all apply --terragrunt-non-interactive",
    "tg-apply-pure": "cd terragrunt/live/dev && terragrunt run-all apply --terragrunt-non-interactive",
    "tg-destroy": "cd terragrunt/live/dev && terragrunt run-all destroy --terragrunt-non-interactive",
    "zip": "cd terragrunt/modules/lambda/src && yarn && zip -r ../lambda.zip .",
    "lint": "eslint src/**/*.{ts,tsx}",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "test": "jest --coverage=false",
    "test-cover": "jest --coverage=true",
    "test-open": "jest --coverage=true && open coverage/lcov-report/index.html",
    "integration-dev": "jest jest/integration/integration.test.ts --coverage=false --modulePathIgnorePatterns=[]",
    "cy": "cypress run --browser chrome",
    "cy-dev": "cypress run --browser chrome --env domain=\"http://dev4.annapivunova.me.s3-website.eu-central-1.amazonaws.com\"",
    "cy-preprod": "cypress run --browser chrome --env domain=\"http://preprod1.annapivunova.me.s3-website.eu-central-1.amazonaws.com\"",
    "cy-prod": "cypress run --browser chrome --env domain=\"http://annapivunova.me.s3-website.eu-central-1.amazonaws.com\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-favicon": "^1.0.1",
    "react-router-dom": "^6.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@jest/globals": "^29.4.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "axios": "^1.3.4",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "cypress": "^12.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "path": "^0.12.7",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
