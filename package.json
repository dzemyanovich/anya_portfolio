{
  "name": "any_portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:dzemyanovich/anya_portfolio.git",
  "author": "Aliaksei Dzemyanovich <aliaksei.dzemyanovich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "start": "yarn prod",
    "dev": "webpack-dev-server --mode=development --config webpack.dev.js --open",
    "preprod": "webpack-dev-server --mode=production --config webpack.preprod.js --open",
    "prod": "webpack-dev-server --mode=production --config webpack.prod.js --open",
    "build": "yarn build-prod",
    "build-dev": "webpack --mode=development --config webpack.dev.js",
    "build-preprod": "webpack --mode=production --config webpack.preprod.js",
    "build-prod": "webpack --mode=production --config webpack.prod.js",
    "deploy": "yarn build && aws s3 sync dist/ s3://anna-pivunova --acl public-read",
    "tf-init": "cd terraform && terraform init",
    "plan": "cd terraform && terraform plan",
    "apply": "cd terraform && terraform apply -auto-approve -lock=false",
    "destroy": "cd terraform && terraform destroy",
    "clean": "rm terraform/terraform.tfstate && rm terraform/terraform.tfstate.backup && rm terraform/.terraform.lock.hcl",
    "lint": "eslint src/**/*.jsx src/**/*.js",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "cypress": "cypress open"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-favicon": "^1.0.1",
    "react-router-dom": "^6.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "cypress": "^12.1.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "path": "^0.12.7",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
